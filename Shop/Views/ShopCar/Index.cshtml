@model ShopCartViewModel
<div class="container">
    @foreach(var element in Model.shopCart.listShopItem)
    {
        <div class="alert alert-warning mt-3"  >
            <b>Car: </b>@element.car.name<br/>
            <b>Price: </b>@element.car.price.ToString("c")
        </div>
    }
    <hr/>
    <a class="btn btn-danger " asp-controller="Order" asp-action="Checkout" >Pay </a>
</div>